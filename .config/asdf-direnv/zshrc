### Do not edit. This was autogenerated by 'asdf direnv setup' ###

# Reuse the existing override when it already points to an executable.
_asdf_direnv_bin="${ASDF_DIRENV_BIN:-}"

# Prefer whatever version `asdf current direnv` reports.
if [[ -z "$_asdf_direnv_bin" || ! -x "$_asdf_direnv_bin" ]]; then
  if command -v asdf >/dev/null 2>&1; then
    _asdf_direnv_version="$(asdf current direnv 2>/dev/null | command awk 'NR==1 {print $2}')"
    if [[ -n "$_asdf_direnv_version" && "$_asdf_direnv_version" != "system" ]]; then
      _asdf_direnv_prefix="$(asdf where direnv "$_asdf_direnv_version" 2>/dev/null)"
      if [[ -n "$_asdf_direnv_prefix" ]]; then
        _asdf_direnv_bin="${_asdf_direnv_prefix}/bin/direnv"
      fi
    fi
  fi
fi

if [[ -n "$_asdf_direnv_bin" && -x "$_asdf_direnv_bin" ]]; then
  export ASDF_DIRENV_BIN="$_asdf_direnv_bin"
  eval "$($ASDF_DIRENV_BIN hook zsh)"
else
  print -u2 "[asdf-direnv] Unable to locate a direnv binary installed via asdf"
fi
